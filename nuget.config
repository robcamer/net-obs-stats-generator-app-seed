<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
     <clear />
	 <add key="nuget" value="https://api.nuget.org/v3/index.json" />
	 <add key="github" value="https://nuget.pkg.github.com/%GITHUB_PACKAGE_REGISTRY_ORG_NAME%/index.json" />
  </packageSources>
  <packageSourceCredentials>
     <github>
         <add key="Username" value="%GITHUB_PACKAGE_REGISTRY_USERNAME%" />
         <add key="ClearTextPassword" value="%GITHUB_PACKAGE_REGISTRY_PASSWORD%" />
     </github>
  </packageSourceCredentials>
</configuration>
